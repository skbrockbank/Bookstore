@model Book

<tr>
    <td>@Model.BookID</td>
    <td>@Model.Title</td>
    <td>@Model.AuthorFirstName @Model.AuthorMiddleName @Model.AuthorLastName</td>
    <td>@Model.Publisher</td>
    <td>@Model.ISBN</td>
    <td>@Model.Classification</td>
    <td>@Model.Category</td>
    <td>$@Model.Price</td>
    <td>@Model.Pages</td>
</tr>